<% if current_user.nil? %>
	<h1>Hello! Welcome to Battleships.</h1>
	<h3>Log in to start playing.</h3>
<% else %>
	<h1>Hey, welcome <%= current_user.email %></h1>

<h3>Other users:</h3>
<% User.all.each do |user|%>
	<li><%= user.email %></li>
	<li><%= user.last_sign_in_at %></li>
<% end %>

<% end %>

